{
  "name": "@prorenata/vue-rest-resource",
  "version": "0.8.10",
  "description": "Rest resource management for Vue.js and Vuex projects",
  "homepage": "https://github.com/ProReNata/VueRestResource",
  "repository": {
    "type": "git",
    "url": "https://github.com/ProReNata/VueRestResource.git"
